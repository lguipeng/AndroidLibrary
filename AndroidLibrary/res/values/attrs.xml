<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeToDismissListView" >
        <attr name="dismissHeight" format="reference|integer"/>
        <attr name="dismissWith" format="reference|integer"/>
    </declare-styleable>
    <declare-styleable name="PullToRefreshListView">
        <attr name="ptrMode">
            <flag name="normal" value="0x00"/>
            <flag name="auto" value="0x01"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="PowerImageView">
        <attr name="auto_play" format="boolean"/>
        <attr name="repeat_play" format="boolean"/>
    </declare-styleable>
</resources>